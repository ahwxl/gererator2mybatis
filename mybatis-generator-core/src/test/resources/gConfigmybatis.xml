<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC
 "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
 "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <context id="FlatJava5" targetRuntime="MyBatis3" defaultModelType="flat">
    <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />

    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql://localhost:3306/deep"
        userId="root"  password="123456"/>

    <javaModelGenerator targetPackage="com.bplow.deep.bpm.domain"
        targetProject="target/">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>
    
    <sqlMapGenerator targetPackage="com.bplow.deep.bpm.mapper"  targetProject="target/">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>
   
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.bplow.deep.bpm.mapper"
     	targetProject="target/">
    </javaClientGenerator>

    <table tableName="auto_app_info" />
   
  </context>
</generatorConfiguration>
