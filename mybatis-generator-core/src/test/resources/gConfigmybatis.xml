<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC
 "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
 "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <context id="FlatJava5" targetRuntime="MyBatis3" defaultModelType="flat">
    <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />

    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
        connectionURL="jdbc:mysql://localhost:3306/ads"
        userId="root"  password="123456"/>

    <javaModelGenerator targetPackage="ibatortest.model"
        targetProject="target/">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>
    
    <sqlMapGenerator targetPackage="ibatortest.mapper"  targetProject="target/">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>
   
    <javaClientGenerator type="XMLMAPPER" targetPackage="ibatortest.client"
     	targetProject="target/">
    </javaClientGenerator>

    <table tableName="sys_menu" />
   
  </context>
</generatorConfiguration>
